<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rbworks.dev.cquiz.com.rbwdcq">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="C Quiz "
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.rbworks.dev.cquiz.com.rbwdcq.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".MainActivity"></activity>
        <activity
            android:name=".MainAbout"
            android:label="About Us "
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.rbworks.dev.cquiz.com.rbwdcq.MainActivity" />

        <activity
            android:name=".QuizActivity"
            android:label="C Quiz "
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.rbworks.dev.cquiz.com.rbwdcq.MainActivity" />


        <activity
            android:name=".ResultActivity"
            android:label="Result "
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.rbworks.dev.cquiz.com.rbwdcq.MainActivity" />





    </application>


</manifest>